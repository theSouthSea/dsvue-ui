{
  "name": "dsvue-ui2",
  "description": "基于vue的管理端组件库",
  "main": "dist/index.umd.js",
  "keywords": [
    "dsvue-ui",
    "vue",
    "ui"
  ],
  "author": "ds",
  "license": "ISC",
  "files": [
    "dist",
    "components"
  ],
  "version": "0.2.1",
  "scripts": {
    "dev": "vite",
    "build-vite": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:css": "npx gulp sass",
    "build:js": "webpack --config ./webpack.component.js",
    "build": "npm run build:js && npm run build:css",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "bash deploy.sh",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "cross-env HUSKY_SKIP_HOOKS=1 standard-version",
    "test": "vitest",
    "build:lib": "vite build --config ./build/lib.config.js"
  },
  "lint-staged": {
    "components/**/*": "prettier --write --ignore-unknown",
    "src/**/*": "prettier --write --ignore-unknown",
    "*.{vue, js, ts}": [
      "eslint --quiet --fix"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
